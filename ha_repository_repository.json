{
  "name": "George Călugăr's Home Assistant Integrations",
  "url": "https://github.com/CMGeorge/ha_repository",
  "maintainer": "George Călugăr (@cmgeorge)",
  "description": "Central repository for Home Assistant custom integrations by George Călugăr, including EPEVER Hi solar controllers and Sabiana Smart Energy devices.",
  "version": "1.0.0",
