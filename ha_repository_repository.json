  "repository_type": "integration",
  "integrations": [
    {
      "name": "EPEVER Hi",
      "description": "Monitor and control EPEVER Hi solar charge controllers via Modbus RTU or TCP",
      "domain": "epever_hi",
      "version": "1.0.0",
      "documentation": "https://github.com/CMGeorge/HomeAssistant_EPEVER_Hi",
      "issue_tracker": "https://github.com/CMGeorge/HomeAssistant_EPEVER_Hi/issues",
      "codeowners": ["@cmgeorge"],
      "iot_class": "local_polling",
      "config_flow": true,
      "integration_type": "hub",
      "requirements": ["pymodbus>=3.0.0"],
      "dependencies": ["modbus"],
      "download_url": "https://github.com/CMGeorge/HomeAssistant_EPEVER_Hi/archive/main.zip",
      "zip_release": true,
      "filename": "epever_hi.zip"
    },
    {
      "name": "Sabiana Smart Energy",
      "description": "Integration for Sabiana Smart Energy devices",
      "domain": "sabiana_smart_energy",
      "version": "1.0.0",
      "documentation": "https://github.com/CMGeorge/homeassistant_sabiana_smart_energy",
      "issue_tracker": "https://github.com/CMGeorge/homeassistant_sabiana_smart_energy/issues",
      "codeowners": ["@cmgeorge"],
      "iot_class": "local_polling",
      "config_flow": true,
      "integration_type": "device",
      "requirements": [],
      "dependencies": [],
      "download_url": "https://github.com/CMGeorge/homeassistant_sabiana_smart_energy/archive/main.zip",
      "zip_release": true,
      "filename": "sabiana_smart_energy.zip"
    }
  ]
}